(function () {
    'use strict';

    angular
        .module('mphasApp')
        .factory('meterReadingsFactory', meterReadingsFactory);

    function meterReadingsFactory($resource, BASE_API_URL) {
        return $resource(BASE_API_URL + 'MeterReadings/:entryId', {
            entryId: '@id'
        }, {
            'delete': {
                method: 'DELETE'
            }
        });
    }
})();